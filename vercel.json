{
  "buildCommand": "vite build --config vite.config.prod.ts",
  "outputDirectory": "dist/spa",
  "installCommand": "pnpm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}