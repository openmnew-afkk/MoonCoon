{
  "version": 2,
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/serverless"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/serverless.ts": {
      "runtime": "nodejs20.x"
    }
  }
}